<Window x:Class="practic3.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:practic3"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Icon="/Resources/folder_file_9761.ico">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="60"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0" Background="#FF76E383" Margin="0,0,0,0" Height="70">
            <Image Width="45" Height="45" HorizontalAlignment="Left" Margin="5" Source="/Resources/photo_2024-09-12_13-55-16.jpg"/>
            <Button Name="btnBack" HorizontalAlignment="Right" Content="Выйти" Margin="670,15,10,15" Click="btnBack_Click"/>
        </Grid>
        <Frame x:Name="FrmMain" Grid.Row="1" NavigationUIVisibility="Hidden" ContentRendered="FrmMain_ContentRendered"/>
    </Grid>
</Window>
